package first;
class A{
	void showA() {
		int a=1;
		System.out.println("This is showA");
	}
	
}
class B extends  A{
	void showB() {
		int b=2;
		System.out.println("This is showB");
	}
}

public class InheritanceClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
System.out.println();
B show=new B();

	}

}
